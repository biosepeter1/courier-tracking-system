{
  "version": 2,
  "env": {
    "VITE_API_BASE_URL": "https://courier-tracking-system-nkdz.onrender.com/api",
    "VITE_SOCKET_URL": "https://courier-tracking-system-nkdz.onrender.com"
  },
  "build": {
    "env": {
      "VITE_API_BASE_URL": "https://courier-tracking-system-nkdz.onrender.com/api",
      "VITE_SOCKET_URL": "https://courier-tracking-system-nkdz.onrender.com"
    }
  },
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
